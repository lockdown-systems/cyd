{
  "name": "cyd",
  "private": true,
  "version": "1.1.21",
  "main": ".vite/build/main.js",
  "description": "Automatically delete your data from tech platforms, except for what you want to keep",
  "license": "proprietary",
  "authors": "Lockdown Systems LLC",
  "scripts": {
    "clean": "node ./scripts/clean.mjs",
    "rebuild": "npx electron-rebuild --force",
    "rebuild-clean": "rm -rf node_modules/better-sqlite3/build && npx electron-rebuild --force",
    "postinstall": "node scripts/postinstall.js",
    "start": "electron-forge start",
    "dev": "electron-forge start",
    "make-local": "node ./scripts/make.js make local",
    "make-dev": "node ./scripts/make.js make dev",
    "make-prod": "node ./scripts/make.js make prod",
    "publish-dev": "node ./scripts/make.js publish dev",
    "publish-prod": "node ./scripts/make.js publish prod",
    "test": "bash -c 'electron --no-sandbox node_modules/vitest/vitest.mjs run --coverage --run \"${1:-src}\"' --",
    "test:windows": "electron --no-sandbox node_modules/vitest/vitest.mjs run --coverage --run src",
    "test:ci": "electron --no-sandbox node_modules/vitest/vitest.mjs run . --coverage",
    "lint": "prettier . --check && eslint --cache --max-warnings=0 && vue-tsc --project src/renderer/tsconfig.json --noEmit",
    "fix": "eslint --fix",
    "format": "prettier --write .",
    "config-open": "cp ./config/open.json ./build/config.json",
    "config-local": "cp ./config/local.json ./build/config.json",
    "config-dev": "cp ./config/dev.json ./build/config.json",
    "config-prod": "cp ./config/prod.json ./build/config.json"
  },
  "dependencies": {
    "@atproto/api": "^0.16.3",
    "@atproto/oauth-client-node": "^0.3.5",
    "@fortawesome/fontawesome-free": "^7.0.0",
    "@rushstack/eslint-patch": "^1.12.0",
    "@types/bootstrap": "^5.2.10",
    "@types/jsdom": "^21.1.7",
    "better-sqlite3": "^12.2.0",
    "bootstrap": "^5.3.7",
    "chart.js": "^4.5.0",
    "cheerio": "^1.1.2",
    "date-fns": "^4.1.0",
    "electron-log": "^5.4.3",
    "electron-squirrel-startup": "^1.0.1",
    "extract-zip": "^2.0.1",
    "glob": "^11.0.3",
    "http-encoding": "^2.1.1",
    "http-mitm-proxy": "^1.1.0",
    "jsdom": "^26.1.0",
    "marked": "^16.2.0",
    "mhtml2html": "^3.0.0",
    "mime-types": "^3.0.1",
    "mitt": "^3.0.1",
    "moment": "^2.30.1",
    "semver": "^7.7.2",
    "unzipper": "^0.12.3",
    "vue": "^3.5.19",
    "vue-chartjs": "^5.3.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "7.4.0",
    "@electron-forge/maker-deb": "7.4.0",
    "@electron-forge/maker-dmg": "7.4.0",
    "@electron-forge/maker-rpm": "7.4.0",
    "@electron-forge/maker-squirrel": "7.4.0",
    "@electron-forge/maker-zip": "7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "7.4.0",
    "@electron-forge/plugin-fuses": "7.4.0",
    "@electron-forge/plugin-vite": "7.4.0",
    "@electron-forge/publisher-s3": "7.4.0",
    "@electron/fuses": "^1.8.0",
    "@electron/notarize": "^3.0.2",
    "@electron/osx-sign": "^2.0.0",
    "@electron/rebuild": "^4.0.1",
    "@eslint/js": "^9.34.0",
    "@types/archiver": "^6.0.3",
    "@types/better-sqlite3": "^7.6.13",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/jest": "^30.0.0",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.3.0",
    "@types/node-fetch": "^2.6.13",
    "@types/semver": "^7.7.0",
    "@types/unzipper": "^0.10.11",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "archiver": "^7.0.1",
    "cross-env": "^10.0.0",
    "electron": "^37.3.1",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.4.0",
    "happy-dom": "^18.0.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.3",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.6"
  },
  "optionalDependencies": {
    "appdmg": "^0.6.6"
  },
  "workspaces": [
    "archive-static-sites/x-archive",
    "archive-static-sites/facebook-archive",
    "docs"
  ]
}
