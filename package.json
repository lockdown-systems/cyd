    "make-dev": "node ./scripts/make.js make dev",
    "make-prod": "node ./scripts/make.js make prod",
    "publish-dev": "node ./scripts/make.js publish dev",
    "publish-prod": "node ./scripts/make.js publish prod",
    "test": "npm rebuild && vitest run . && node ./scripts/clean.js",
    "test:components": "cd src/renderer && cypress run --component",
    "open-cypress": "cd src/renderer && cypress open",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "config-open": "./archive-static-sites/build.sh && cp ./config/open.json ./build/config.json",
